<div class="todo-mapa">
    <div id="map"></div>
    <div class="barra-busqueda">
        <img src="~/img/lupa.png" alt="busqueda">
        <input type="text" id="search-bar" placeholder="Buscar destino...">
    </div>
    <div class="tiempo-zona">
        Tiempo estimado en la zona: 15 minutos
    </div>

    <div class="conjunto-botones-de-accion">
        @if (ViewBag.AutoOcupado)
        {
            <a class="ocupar-espacio" href="@Url.Action("DesocuparEspacio", "Home", new { id_auto = ViewBag.idAutoOcupando })">
                <img src="~/img/auto.png" alt="Desocupar"> Desocupar Espacio
            </a>
        }
            <a class="reportar" href="@Url.Action("Reportar")">
                <img src="~/img/advertencia.png" alt="Advertencia"> Reportar
            </a>
    </div>

<script>
    @ViewBag.Ubicaciones
</script>
