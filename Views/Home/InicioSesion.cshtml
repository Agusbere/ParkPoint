<div class = "div-centrado"> 
<div class = "bg-log-in">
<form class="form" action='@Url.Action("Index")' method="POST">
    <p class="message">INICIA SESIÓN EN PARKPOINT</p>
            
    <label>
        <input autocomplete = "off" required="" placeholder="" type="email" class="input" name = "email">
        <span>Email</span>
    </label> 
     
    <label>
        <input autocomplete = "off" required="" placeholder="" type="password" class="input" name = "contra">
        <span>Contraseña</span>
    </label>
    <button class="submit">Registrarse</button>
    <p class="signin">¿Ya tienes una cuenta? <a href="#">Iniciar Sesion</a> </p>
</form>

</div>

</div>
