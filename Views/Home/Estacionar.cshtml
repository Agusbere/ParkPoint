@{
    ViewData["Title"] = "ESTACIONAR";
}

<div class="contenedor-main">

    <form action="@Url.Action("Index")">

        <div class="casilla-formulario">
            <h5>Calle<span>*</span></h5>
            <input class="input-formulario" type="text" name="calle" placeholder="Ingrese la calle" autocomplete="off" required />
        </div>

        <div class="casilla-formulario">
            <h5>Altura<span>*</span></h5>
            <input class="input-formulario" type="number" name="Altura" placeholder="Ingrese la altura" autocomplete="off" min="1" max="100000" required />
        </div>

        <input type="submit" value="Ocupar lugar">

    </form>

</div>
